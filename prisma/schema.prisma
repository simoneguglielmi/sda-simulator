generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sda.db"
}

model ItemTracking {
  id            String   @id @default(uuid())
  businessName  String
  progressiveId String
  status        String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
